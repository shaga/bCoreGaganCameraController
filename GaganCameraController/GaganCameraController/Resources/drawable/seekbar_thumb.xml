<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item
      android:bottom="1dp"
      android:left="1dp"
      android:right="1dp"
      android:top="1dp" >

    <shape android:shape="oval">
      <size android:height="32dp"
          android:width="32dp" />
      <solid android:color="#000000" />
    </shape>

  </item>

  <item
      android:bottom="2dp"
      android:left="2dp"
      android:right="2dp"
      android:top="2dp" >

    <shape android:shape="oval">
      <size android:height="31dp"
          android:width="31dp" />
      <solid android:color="#ffffff" />
    </shape>

  </item>

</layer-list>
